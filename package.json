{
	"name": "kent-flix-client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-redux": "^5.1.1",
		"react-router-dom": "^4.3.1",
		"react-scripts": "2.0.4",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
